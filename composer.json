  "authors": [
    {
      "name": "Jaisen Mathai",
      "email": "jaisen@jmathai.com",
      "homepage": "http://jaisenmathai.com/"
    }
  ],
  "support": {
    "issues": "https://github.com/jmathai/s3-bucket-stream-zip-php/issues"
  },
  "require": {
